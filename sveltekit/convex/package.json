{
  "name": "vibe-starter-sveltekit",
  "version": "1.0.0",
  "description": "SvelteKit starter with Convex, TailwindCSS, and TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "cf:preview": "vite build && wrangler dev",
    "cf:deploy": "vite build && wrangler deploy"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.90",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "convex": "^1.31.4",
    "convex-svelte": "^0.0.12",
    "creem_io": "^0.4.0",
    "lucide-svelte": "^0.562.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@internationalized/date": "^3.10.1",
    "@lucide/svelte": "^0.562.0",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/node": "^25.0.8",
    "autoprefixer": "^10.4.23",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "formsnap": "^2.0.1",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "layerchart": "2.0.0-next.43",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.46.3",
    "svelte-check": "^4.3.5",
    "svelte-sonner": "^1.0.7",
    "sveltekit-superforms": "^2.29.1",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.1"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "pnpm": {
    "overrides": {
      "cookie": ">=0.7.0"
    }
  }
}
