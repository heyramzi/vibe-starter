{
  "name": "vibe-starter-sveltekit",
  "version": "1.0.0",
  "description": "SvelteKit starter with Convex, TailwindCSS, and TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome check .",
    "cf:preview": "vite build && wrangler dev",
    "cf:deploy": "vite build && wrangler deploy",
    "check:sections": "bash scripts/check-svelte-sections.sh",
    "prepare": "cp scripts/pre-commit .git/hooks/pre-commit 2>/dev/null || true"
  },
  "dependencies": {
    "@convex-dev/better-auth": "latest",
    "@mmailaender/convex-better-auth-svelte": "latest",
    "better-auth": "1.4.19",
    "bits-ui": "^2.16.2",
    "clsx": "^2.1.1",
    "convex": "^1.32.0",
    "convex-svelte": "^0.0.12",
    "creem_io": "^1.0.0",
    "lucide-svelte": "^0.575.0",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@internationalized/date": "^3.11.0",
    "@lucide/svelte": "^0.575.0",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-cloudflare": "^7.2.8",
    "@sveltejs/kit": "^2.53.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/postcss": "^4.2.1",
    "@tailwindcss/vite": "^4.2.1",
    "@tanstack/table-core": "^8.21.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/node": "^25.3.2",
    "autoprefixer": "^10.4.27",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "jsdom": "^28.1.0",
    "layerchart": "2.0.0-next.43",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "postcss": "^8.5.6",
    "svelte": "^5.53.5",
    "svelte-check": "^4.4.4",
    "svelte-sonner": "^1.0.7",
    "sveltekit-superforms": "^2.30.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.69.0"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "pnpm": {
    "overrides": {
      "cookie": ">=0.7.0"
    }
  }
}
